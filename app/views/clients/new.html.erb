<%= form_with model: @client  do |form| %>
	<div>
        <%= form.label :user_name %> <br>
        <%= form.text_field :user_name %> <br>
    </div>

    <div>
        <%= form.label :password %> <br>
        <%= form.text_field :password %> <br>
    </div>

    <div>
        <%= form.submit %>
    </div>
 <% end %>
